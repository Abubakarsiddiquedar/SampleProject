<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <EnableRequestDecompression>true</EnableRequestDecompression>
    <UserSecretsId>Innovo-Catalog-Api-Dev-Secret</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" />
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Application\Handlers\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Innovo.Catalog.Contracts\Innovo.Catalog.Contracts.csproj" />
  </ItemGroup>
</Project>
